{
  "name": "vite-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npx serve -s ./dist -l 8080",
    "dev": "vite",
    "dev:local": "vite --mode local-api",
    "build": "tsc && vite build",
    "preview": "npm run build && vite preview",
    "lint": "eslint -c .eslintrc.json . --ext .tsx",
    "lint-and-fix": "eslint -c .eslintrc.json . --ext .tsx --fix",
    "prettier-format": "prettier --config .prettierrc  --write .",
    "test": "jest",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint -c .eslintrc.json . --ext .tsx --fix",
      "prettier --config .prettierrc  --write ."
    ]
  },
  "dependencies": {
    "@azure/storage-blob": "^12.11.0",

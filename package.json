    "@heroicons/react": "^1.0.6",
    "axios": "^1.1.2",
    "clsx": "^1.2.1",
    "echarts": "^5.4.0",
    "echarts-for-react": "^3.0.2",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "match-sorter": "^6.3.1",
    "moment": "^2.29.4",
    "pm2": "^5.2.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
